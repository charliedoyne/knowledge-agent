# ===================
# GCP Configuration
# ===================
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=europe-west2

# Agent Engine ID (only needed for production, not local development)
AGENT_ENGINE_ID=your-agent-engine-id

# ===================
# Local Development
# ===================
# Set to true to run agent locally instead of via Agent Engine
LOCAL_MODE=true

# Dev user identity (for testing PR attribution without IAP)
DEV_USER_EMAIL=charlie@datatonic.com
DEV_USER_NAME=Charlie

